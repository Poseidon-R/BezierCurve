<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.zhangxiangzeng.beziercurveproject.MainActivity">

    <TextView
        android:id="@+id/toPointView"
        style="@style/mainBtnStyle"
        android:text="@string/usePointViewTag"
        android:layout_centerInParent="true"/>

    <TextView
        android:id="@+id/toWaveView"
        style="@style/mainBtnStyle"
        android:text="@string/useWaveViewTag"
        android:layout_below="@id/toPointView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"/>

</RelativeLayout>
